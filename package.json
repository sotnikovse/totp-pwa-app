{
  "name": "totp-pwa-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:wasm": "wasm-pack build ./wasm --target web",
    "preview": "vite preview",
    "typecheck": "vue-tsc -b",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --ignore-unknown --list-different --write .",
    "generate:pwa-assets": "pwa-assets-generator"
  },
  "devDependencies": {
    "@types/node": "^22.19.0",
    "@vite-pwa/assets-generator": "^1.0.2",
    "lightningcss": "^1.30.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-sfce": "^0.2.4",
    "vitest": "^4.0.7",
    "vue-tsc": "^3.1.3"
  }
}
